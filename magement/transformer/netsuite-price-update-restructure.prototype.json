{
    "$schema": "https://di.schema.mediact.com/register.prototype.json",
    "type": "list-transformer",
    "identifier": "netsuite-price-update-restructure",
    "name": "Netsuite - Product Price Update Restructure",
    "object": {
      "prototype": "chain",
      "parameters": {
        "transformers": [
          {
            "prototype": "node",
            "parameters": {
              "accessor": {
                "prototype": "pattern",
                "parameters": {
                  "pattern": "items.*"
                }
              },
              "nodeTransformers": [
                {
                  "prototype": "key-filter",
                  "parameters": {
                    "accessor": {
                      "prototype": "key",
                      "parameters": {
                        "keys": [
                          "links",
                          "lastmodifieddate"
                        ]
                      }
                    }
                  }
                },
                {
                  "prototype": "key-mapper",
                  "parameters": {
                    "mappers": [
                      {
                        "prototype": "dictionary-map",
                        "parameters": {
                          "map": [
                            [
                              "id",
                              "identifier"
                            ],
                            [
                              "itemid",
                              "sku"
                            ],
                            [
                              "pricelevelname",
                              "description"
                            ],
                            [
                              "currency",
                              "priceCurrency"
                            ]
                          ],
                          "comparator": "equals"
                        }
                      }
                    ],
                    "accessor": {
                      "prototype": "pattern",
                      "parameters": []
                    }
                  }
                }
              ]
            }
          },
          {
            "prototype": "brancher",
            "parameters": {
              "pattern": "items.*",
              "entityType": "netsuite-product-price"
            }
          }
        ]
      }
    }
  }